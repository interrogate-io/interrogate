{
  "name": "@interrogate/remark-plugin-plantuml",
  "version": "1.0.0",
  "license": "MIT",
  "repository": {
    "directory": "libs/remark-plugin-plantuml",
    "type": "git",
    "url": "https://github.com/interrogate-io/interrogate"
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "module": "./dist/index.js",
      "import": "./dist/index.js",
      "require": "./dist/cjs/index.cjs"
    }
  },
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/*",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "npm run build-esm && npm run build-cjs",
    "build-esm": "tsc --project tsconfig.build.json",
    "build-cjs": "esbuild src/index.ts --bundle --packages=external --platform=node --format=cjs --outfile=dist/cjs/index.cjs",
    "check-dependencies": "depcruise src",
    "format": "prettier . --write",
    "lint": "eslint .",
    "test": "vitest run --coverage",
    "test-ui": "vitest --ui",
    "typecheck": "tsc --project tsconfig.json --noEmit"
  },
  "dependencies": {
    "@interrogate/plantuml-to-svg": "1.0.0",
    "to-vfile": "8.0.0",
    "unist-util-visit": "5.0.0"
  },
  "devDependencies": {
    "@types/mdast": "4.0.4",
    "remark": "15.0.1",
    "vfile": "6.0.3",
    "vitest": "3.2.4"
  }
}
