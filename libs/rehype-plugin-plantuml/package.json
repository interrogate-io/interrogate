{
  "name": "@interrogate/rehype-plugin-plantuml",
  "version": "1.1.0",
  "license": "MIT",
  "repository": {
    "directory": "libs/rehype-plugin-plantuml",
    "type": "git",
    "url": "https://github.com/interrogate-io/interrogate"
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "module": "./dist/index.js",
      "import": "./dist/index.js",
      "require": "./dist/cjs/index.cjs"
    }
  },
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/*",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "npm run build-esm && npm run build-cjs",
    "build-esm": "tsc --project tsconfig.build.json",
    "build-cjs": "esbuild src/index.ts --bundle --packages=external --platform=node --format=cjs --outfile=dist/cjs/index.cjs",
    "check-dependencies": "depcruise src",
    "format": "prettier . --write",
    "lint": "eslint .",
    "test": "vitest run --coverage",
    "test-ui": "vitest --ui",
    "typecheck": "tsc --project tsconfig.json --noEmit"
  },
  "dependencies": {
    "@interrogate/plantuml-to-svg": "1.1.0",
    "hast-util-from-html": "2.0.3",
    "to-vfile": "8.0.0",
    "unist-util-visit": "5.0.0"
  },
  "devDependencies": {
    "@interrogate/remark-plugin-plantuml": "1.1.0",
    "@types/hast": "3.0.4",
    "rehype-stringify": "10.0.1",
    "remark-parse": "11.0.0",
    "remark-rehype": "11.1.2",
    "unified": "11.0.5",
    "vfile": "6.0.3",
    "vitest": "3.2.4"
  }
}
